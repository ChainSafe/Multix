{
  "overwrite": true,
  "schema": "https://kusama.multix.chainsafe.io/graphql",
  "documents": "src/**/*.graphql",
  "generates": {
    "src/gql/": {
      "preset": "client",
      "plugins": []
    },
    "types-and-hooks.tsx": {
      "plugins": ["typescript", "typescript-operations", "typescript-react-query"],
      "config": {
        "fetcher": "./src/fetcher#fetchData"
      }
    }
  }
}
