{
  "name": "multix-ui",
  "version": "0.1.0",
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@mui/styled-engine": "^5.12.3",
    "@polkadot/api": "^10.4.1",
    "@polkadot/extension-dapp": "^0.46.1",
    "@polkadot/react-identicon": "^3.2.1",
    "@polkadot/util-crypto": "^12.0.1",
    "@tanstack/react-query": "^4.29.3",
    "@types/react-router-dom": "^5.3.3",
    "graphql": "^15.8.0",
    "graphql-request": "^6.0.0",
    "graphql-ws": "^5.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query-subscription": "2.0.0-beta.1",
    "react-router-dom": "^6.11.1",
    "reactflow": "^11.7.0",
    "rxjs": "^7.8.0",
    "typescript": "5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/client-preset": "3.0.1",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@graphql-eslint/eslint-plugin": "^3.18.0",
    "@polkadot/typegen": "^10.4.1",
    "@types/node": "^18.16.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "start": "vite --port 3333",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:types": "tsc --pretty",
    "lint:fix": "yarn run lint --fix",
    "codegen": "graphql-codegen --config graphql.config.json",
    "generate:type-from-defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --endpoint ./node-metadata.json --package src/interfaces --input ./src/interfaces",
    "generate:types-from-chain": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --endpoint wss://rococo-rpc.polkadot.io --output ./src/interfaces"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@graphql-typed-document-node/core": "3.2.0"
  }
}