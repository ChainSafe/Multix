{
  "name": "@chainsafe/multix",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "An interface to easily manage complex multisigs.",
  "engines": {
    "node": ">=16.10"
  },
  "packageManager": "yarn@3.5.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChainSafe/Multix"
  },
  "keywords": [
    "polkadot",
    "multisig"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn workspaces foreach run build",
    "lint": "yarn workspaces foreach run lint",
    "start:ui": "yarn workspace multix-ui start",
    "codegen:squid": "yarn workspace multix-squid codegen",
    "build:squid": "yarn workspace multix-squid build",
    "install:squid": "yarn workspaces focus multix-squid"
  },
  "devDependencies": {
    "prettier": "2.8.8"
  }
}
